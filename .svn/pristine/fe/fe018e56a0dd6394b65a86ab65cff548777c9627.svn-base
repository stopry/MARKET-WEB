<div class="loadBox">
  <div class="conWrap">
    <div class="loadLogo">

    </div>
    <form class="loadForm">
      <div class="inputCtr radiu border">
        <i class="leftIcon load_account"></i>
        <input class="input right" [(ngModel)]="loginData.account" name="loginAccount"   placeholder="请输入您的帐号" type="number">
      </div>
      <div class="inputCtr radiu border">
        <i class="leftIcon load_pwd"></i>
        <input class="input right" placeholder="请输入您的登录密码" name="loginPassword" [(ngModel)]="loginData.password"  type="password">
      </div>
      <div class="loadBotBox">
        <div class="clause">
          <label>
            <input type="checkbox" class="cluaseCheck" name="clause" [(ngModel)]="isRemPwd"/>
            <span class="clauseCon">记住密码</span>
          </label>
        </div>
        <p class="forgetPwd" routerLink="/forgetPassword">忘记密码？</p>
      </div>
      <div feedButton class="loadBtn btn keyBtn" (click)="logIn()">
        登录
      </div>
      <div class="quickLoadText">

      </div>
      <div class="quickLoadBox">
        <div feedButton class="quickLoadItem WX"></div>
        <div feedButton class="quickLoadItem QQ"></div>
      </div>
    </form>
  </div>
  <div class="toRegistPage" routerLink="/regist">立即注册</div>
</div>
