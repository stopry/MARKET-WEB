<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>登录</title>
  <meta name="viewport" content="width=device-width,user-scalable=no,initial-scale=1, minimum-scale=1,maximum-scale=1"/>
  <meta name="description" content="Z家园，2017最火爆种植游戏。" />
  <meta name="keywords" content="Z家园|环保|绿色|种树|农场|种植" />
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
  <meta name="format-detection" content="telephone=no">
  <meta name="renderer" content="webkit"/>
  <meta name="force-rendering" content="webkit"/>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
  <meta name="msapplication-tap-highlight" content="no">
  <meta name="full-screen" content="yes"/>
  <meta name="x5-fullscreen" content="true"/>
  <meta name="360-fullscreen" content="true"/>
  <meta name="screen-orientation" content=""/>
  <meta name="x5-orientation" content="">
  <meta name="browsermode" content="application">
  <meta name="x5-page-mode" content="app">
  <link rel="icon" type="image/x-icon" href="../assets/images/favicon.ico">
  <script src="../assets/js/plugin/flexible.css.js"></script>
  <script src="../assets/js/plugin/flexible.ios.js"></script>
  <link href="../assets/css/reset.css" rel="stylesheet">
  <link href="../assets/css/global.css" rel="stylesheet">
</head>
<body>
<!--.hide 隐藏注册界面-->
<div class="registBox">
  <div class="header">
    <i class="leftSwordIcon feedbtn" onclick="window.history.back()"></i>
    <h3 class="headerTitle">注册</h3>
  </div>
  <div class="regBanner">

  </div>
  <form class="regForm">
    <div class="conWrap">
      <div class="inputCtr borderBot">
        <i class="leftIcon reg_mobile"></i>
        <input class="input right" autocomplete="off"  name="mobile" placeholder="请输入手机号码" type="text">
      </div>

      <div class="inputCtr borderBot getVerCode">
        <i class="leftIcon reg_capcode"></i>
        <input class="input right" name="msgCode" placeholder="请输入图形验证码" type="text">
        <div class="rightOpr getCapCode feedbtn">
          <img src="../assets/images/erw_code.png" style="width: 100%;height: 0.9425rem !important;"  alt="">
        </div>
      </div>
      <div class="inputCtr borderBot getMsgCode">
        <i class="leftIcon reg_vercode"></i>
        <input class="input right" name="msgCode" placeholder="请输入短信验证码" type="number">
        <!--.disable 此按钮不可点击-->
        <div class="getVerCode feedbtn">
          获取验证码
        </div>
      </div>
      <div class="inputCtr borderBot">
        <i class="leftIcon reg_pwd"></i>
        <input class="input right" name="pwd" placeholder="请输入密码" type="password">
      </div>
      <div class="inputCtr borderBot">
        <i class="leftIcon reg_pwdagain"></i>
        <input class="input right" name="pwdAgain" placeholder="请再次输入密码" type="password">
      </div>
      <div class="inputCtr borderBot">
        <i class="leftIcon reg_prtcode"></i>
        <input class="input right" name="superId"  placeholder="请填写您的推荐码(非必填)" type="text">
      </div>
      <div class="clause">
        <label>
          <input type="checkbox" class="cluaseCheck" name="clause"/>
          <span class="clauseCon">同意Z家园</span>
        </label>
        <i class="feedbtn">《服务条款》</i>
      </div>
      <div  class="regBtn btn keyBtn feedbtn">
        注册
      </div>
      <p class="hasAccount feedbtn" onclick="location='/html/login.html'">已有账号现在登录</p>
    </div>
  </form>
</div>
<!--.active 显示这个界面-->
<div class="regSuccess">
  <div class="tipsImg">

  </div>
  <div class="nowLoadBtb feedbtn" onclick="location='/html/login.html'">
    立即登陆
  </div>
</div>
<!--服务条框 .hide 隐藏-->
<div class="readcClauseBox hide">
  <div class="clauseHeader">
    Z家园服务条款
    <i class="close feedbtn">
      ×
    </i>
  </div>
  <div class="clauseBoxCon">
    服务条框服<br/>
    服务条框服
  </div>
  <div class="okClose feedbtn">
    确定
  </div>
</div>
<script src="../assets/js/lib/zepto.js"></script>
<script src="../assets/js/plugin/layer/layer.js"></script>
<script src="../assets/js/myModule.js"></script>
<script src="../assets/js/global/global.js"></script>
<script src="../assets/js/global.js"></script>
<script>
  $(function () {
      objVerticalCenter(".readcClauseBox");
  })
</script>
</body>
</html>

