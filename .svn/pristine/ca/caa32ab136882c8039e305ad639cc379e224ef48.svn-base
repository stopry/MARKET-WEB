<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <title>上市下架</title>
  <meta name="viewport" content="width=device-width,user-scalable=no,initial-scale=1, minimum-scale=1,maximum-scale=1"/>
  <meta name="description" content="Z家园，2017最火爆种植游戏。" />
  <meta name="keywords" content="Z家园|环保|绿色|种树|农场|种植" />
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
  <meta name="format-detection" content="telephone=no">
  <meta name="renderer" content="webkit"/>
  <meta name="force-rendering" content="webkit"/>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
  <meta name="msapplication-tap-highlight" content="no">
  <meta name="full-screen" content="yes"/>
  <meta name="x5-fullscreen" content="true"/>
  <meta name="360-fullscreen" content="true"/>
  <meta name="screen-orientation" content=""/>
  <meta name="x5-orientation" content="">
  <meta name="browsermode" content="application">
  <meta name="x5-page-mode" content="app">
  <link rel="icon" type="image/x-icon" href="../assets/images/favicon.ico">
  <script src="../assets/js/plugin/flexible.css.js"></script>
  <script src="../assets/js/plugin/flexible.ios.js"></script>
  <link href="../assets/css/reset.css" rel="stylesheet">
  <link href="../assets/css/global.css" rel="stylesheet">
</head>
<body style="background: #f8f8f8">
<div class="header">
  <i class="leftSwordIcon feedbtn" onclick="window.history.back()"></i>
  <h3 class="headerTitle">上市下架</h3>
</div>
<div class="grayBgBar mt80Rem"></div>
<div class="noCon hide">
  没有任何内容
</div>
<ul class="listedList" id="ul"></ul>
<div class="botOprArea">
  <div class="oprBtn guaPai keyColorBg feedbtn">上市</div>
  <div class="oprBtn zaiPai upColorBg feedbtn">下架</div>
</div>

<input type="hidden"  id="itemcnt">
<input type="hidden"  id="minTran">
<!--上市弹框-->
<div class="paiBox gau">
  <div class="boxHeader">
    <span class="fl">上市</span>
    <i class="closeThis fr feedbtn">

    </i>
  </div>
  <div class="boxCon">
    <div class="inputArea">
      <span class="iptTitle fl">商品</span>
      <div class="iptDiv fr">
        <input type="text" disabled class="proIpt" id="g_greenName">
        <input type="hidden"  id="g_greenId">
        <div class="selAdon selAdonGreen feedbtn">
          选择
        </div>
      </div>
    </div>
    <div class="inputArea">
      <span class="iptTitle fl">数量</span>
      <div class="iptDiv num fr">
        <div class="infiOpr min fl feedbtn">
          MIN
        </div>
        <div class="selAdon rscNum feedbtn">
          -
        </div>
        <input type="number" class="proIpt num" id="g_cnt">
        <div class="selAdon addNum feedbtn">
          +
        </div>
        <div class="infiOpr max fl feedbtn">
          MAX
        </div>
      </div>
    </div>
    <div class="inputArea">
      <span class="iptTitle fl">交易密码</span>
      <div class="iptDiv fr">
        <input type="password" class="proIpt pwd" id="g_password">
      </div>
    </div>
    <div class="confirmBtn feedbtn keyColorBg " onclick="guazhai(1)"> 上市 </div>
  </div>
</div>
<!--下架弹框-->
<div class="paiBox zai">
  <div class="boxHeader">
    <span class="fl">下架</span>
    <i class="closeThis fr feedbtn">

    </i>
  </div>
  <div class="boxCon">
    <div class="inputArea">
      <span class="iptTitle fl">商品</span>
      <div class="iptDiv fr">
      <input type="text" disabled class="proIpt" id="z_greenName">
        <input type="hidden"  id="z_greenId">
        <div class="selAdon selAdonGreen feedbtn">
          选择
        </div>
      </div>
    </div>
    <div class="inputArea">
      <span class="iptTitle fl">数量</span>
      <div class="iptDiv num fr">
        <div class="infiOpr min fl feedbtn">
          MIN
        </div>
        <div class="selAdon rscNum feedbtn">
          -
        </div>
        <input type="number" class="proIpt num" id="z_cnt">
        <div class="selAdon addNum feedbtn">
          +
        </div>
        <div class="infiOpr max fl feedbtn">
          MAX
        </div>
      </div>
    </div>
    <div class="inputArea">
      <span class="iptTitle fl">交易密码</span>
      <div class="iptDiv fr">
        <input type="password" class="proIpt pwd" id="z_password">
      </div>
    </div>
    <div class="confirmBtn feedbtn" onclick="guazhai(2)"> 下架 </div>
  </div>
</div>
<!--商品选择框-->
<div class="listedSelProBox">
  <div class="proHeader">
    <img class="imgTitle" src="../assets/images/sel_grenn_top_title.png" alt="">
    <div class="closeThis feedbtn"></div>
  </div>
  <div class="proCon">
    <div class="greenSelBox" id="goods">
    </div>
    <div class="confirmBtn feedbtn" id="selGreen">确定</div>
  </div>
</div>
<script src="../assets/js/lib/zepto.js"></script>
<script src="../assets/js/plugin/layer/layer.js"></script>
<script src="../assets/js/myModule.js"></script>
<script src="../assets/js/global/global.js"></script>
<script src="../assets/js/global.js"></script>
<script src="../assets/js/listed-market.js"></script>
</body>
</html>


